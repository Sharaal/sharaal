<div class="row marketing">
  <div class="col-md-12">
    <h4 class="text-center">
      <a href="mailto:{{ contacts.email }}">{{ contacts.email }}</a>
    </h4>
  </div>
</div>
<div class="row marketing">
  {% set columns = 12 / contacts.links.length %}
  {% for link in contacts.links %}
    <div class="col-xs-{{ columns }}">
      <h4 class="text-center">
        <a href="{{ link.url }}">
          <img src="{{ link.image }}" title="{{ link.title }}" width="50px" height="50px" />
        </a>
      </h4>
    </div>
  {% endfor %}
</div>
